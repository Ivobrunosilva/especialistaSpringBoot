package com.example.algafood;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.ResponseBody;

import com.example.algafood.di.service.AtivacaoClienteService;

@Controller
public class MeuPrimeiroController {
	
	private AtivacaoClienteService AtivacaoClienteService;
	
	
	public MeuPrimeiroController(com.example.algafood.di.service.AtivacaoClienteService ativacaoClienteService) {
		super();
		AtivacaoClienteService = ativacaoClienteService;
	}


	@GetMapping("/hello")
	@ResponseBody
	public String hello() {
		return "Hello";
	}
}
